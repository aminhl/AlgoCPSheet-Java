import java.util.Scanner;
import static java.lang.Math.*;

public class main {
    public static void main (String [] args){
        Scanner sc = new Scanner(System.in);
        long a = sc.nextLong();
        long b = sc.nextLong();
        long c = sc.nextLong();
        long d = sc.nextLong();
        if (b*log(a)>d*log(c))
            System.out.println("YES");
        else
            System.out.println("NO");
    }
}